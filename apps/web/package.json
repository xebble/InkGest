{
  "name": "@inkgest/web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit",
    "test": "jest --passWithNoTests",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "db:generate": "prisma generate --schema=../../packages/database/schema.prisma",
    "db:push": "prisma db push --schema=../../packages/database/schema.prisma",
    "db:migrate": "prisma migrate dev --schema=../../packages/database/schema.prisma",
    "db:studio": "prisma studio --schema=../../packages/database/schema.prisma",
    "clean": "rm -rf .next && rm -rf node_modules"
  },
  "dependencies": {
    "next": "15.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "@prisma/client": "^5.6.0",
    "next-auth": "^4.24.5",
    "next-intl": "^3.3.0",
    "zod": "^3.22.4",
    "zustand": "^4.4.7",
    "tailwindcss": "^3.3.6",
    "autoprefixer": "^10.4.16",
    "postcss": "^8.4.32",
    "clsx": "^2.0.0",
    "class-variance-authority": "^0.7.0",
    "lucide-react": "^0.294.0"
  },
  "devDependencies": {
    "@types/node": "^20.9.0",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "typescript": "^5.2.2",
    "eslint": "^8.54.0",
    "eslint-config-next": "15.0.0",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "tailwindcss-animate": "^1.0.7",
    "prettier": "^3.0.3",
    "jest": "^29.7.0",
    "@types/jest": "^29.5.8",
    "@testing-library/react": "^13.4.0",
    "@testing-library/jest-dom": "^6.1.4",
    "jest-environment-jsdom": "^29.7.0",
    "prisma": "^5.6.0"
  }
}